#include<iostream.h>

class A
{
	
public:
	int a;
	int b;
	int c;
	A(){}
	A(int num1,int num2,int num3){a = num1; b = num2 ;c = num3;}
	put(int num1,int num2,int num3){a = num1; b = num2 ;c = num3;}
	void  Print(){  cout<<a<<b<<c; }
};

void main()
{
	A g;
	g.put(1,2,3);
	g.Print();
}
